<mvc:View xmlns:l="sap.ui.layout" xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns:viz.data="sap.viz.ui5.data"
	xmlns:viz.feeds="sap.viz.ui5.controls.common.feeds" xmlns:viz="sap.viz.ui5.controls" xmlns:chart="sap.suite.ui.commons"
	xmlns:customControl="gitTest.GitTest.customControl" xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml"
	controllerName="gitTest.GitTest.controller.SearchData">
	<Page title="Title">
		<content>
			<customControl:MyHeading text="CustomLable For h1"/>
			<customControl:MyButton text="CustomButton Hover Trigger" hover="onHover"/>
			<Table noDataText="Drop column list items here and columns in the area above" id="table0">
				<items>
					<ColumnListItem type="Active" id="item2">
						<cells>
							<Text text="Row 1 Cell 1" id="text0"/>
							<Text text="Row 1 Cell 2" id="text1"/>
							<Text text="Row 1 Cell 3" id="text2"/></cells>
					</ColumnListItem>
				</items>
				<columns>
					<Column id="column0">
						<header>
							<Label text="Header 1" id="label4"/>
						</header>
					</Column>
					<Column id="column1">
						<header>
							<Label text="Header 2" id="label5"/>
						</header>
					</Column>
					<Column id="column2">
						<header>
							<Label text="Header 3" id="label6"/>
						</header>
					</Column>
				</columns>
			</Table>
			<l:VerticalLayout class="sapUiContentPadding" width="100%">
				<l:content>
					<HBox fitContainer="true" alignItems="Stretch">
						<items>
							<VBox fitContainer="true">
								<layoutData>
									<FlexItemData growFactor="7"/>
								</layoutData>
								<items>
									<HBox>
										<VBox class="sapUiResponsiveMargin">
											<Label text="1" labelFor="str1"/>
											<Input id="str1" type="Text" width="40px" maxLength="3" change="onChange"/>
										</VBox>
										<VBox class="sapUiResponsiveMargin">
											<Label text="2" labelFor="str2"/>
											<Input id="str2" type="Text" width="40px" maxLength="3" change="onChange"/>
										</VBox>
										<VBox class="sapUiResponsiveMargin">
											<Label text="3" labelFor="str3"/>
											<Input id="str3" type="Text" width="40px" maxLength="3" change="onChange"/>
										</VBox>
										<VBox class="sapUiResponsiveMargin">
											<Label text="4" labelFor="str4"/>
											<Input id="str4" type="Text" width="40px" maxLength="3" change="onChange"/>
										</VBox>
									</HBox>
									<HBox/>
								</items>
							</VBox>
							<VBox fitContainer="true">
								<layoutData>
									<FlexItemData growFactor="4"/>
								</layoutData>
								<viz:VizFrame id="idpiechart" width="430px" uiConfig="{applicationSet:'fiori'}" vizType="line"
									vizProperties="{ plotArea:{ dataLabel:{visible: true} }, title:{text:'Flavor-Sales'}, yAxis:{ scale:{ fixedRange:true, minValue:0, maxValue:100 } }, valueAxis:{ title: {visible: false} }, legend:{ visible: false } }"
									selectData="myOnClickHandler" renderComplete="handleRenderComplete">
									<viz:dataset>
										<viz.data:FlattenedDataset id="flattenData" data="{IceCreamModel>/}">
											<viz.data:dimensions>
												<viz.data:DimensionDefinition name="Flavor" value="{IceCreamModel>Flavor}"/>
											</viz.data:dimensions>
											<viz.data:measures>
												<viz.data:MeasureDefinition name="Sales" value="{IceCreamModel>Sales}"/>
											</viz.data:measures>
										</viz.data:FlattenedDataset>
									</viz:dataset>
									<viz:feeds>
										<viz.feeds:FeedItem uid="categoryAxis" type="Dimension" values="Flavor"/>
										<viz.feeds:FeedItem uid="valueAxis" type="Measure" values="Sales"/>
									</viz:feeds>
								</viz:VizFrame>
							</VBox>
						</items>
					</HBox>
				</l:content>
			</l:VerticalLayout>
			<VBox>
				<Button text="test" press="test"/>
			</VBox>
			<VBox>
				<Button text="go to next" press="goNext"/>
			</VBox>
		</content>
	</Page>
</mvc:View>