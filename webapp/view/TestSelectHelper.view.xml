<mvc:View height="100%" controllerName="gitTest.GitTest.controller.TestSelectHelper"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:layout="sap.ui.layout"
	xmlns:form="sap.ui.layout.form"
	xmlns="sap.m">
	<Page title="TestSelectHelper"
		showNavButton="true"
		navButtonPress=".onNavBack">
		<content>
			<layout:Grid defaultSpan="L12 M12 S12">
				<layout:content>
					<form:SimpleForm editable="true" layout="ResponsiveGridLayout" labelSpanM="12" labelSpanL="12" labelSpanXL="12" emptySpanL="6" emptySpanM="6">
						<form:content>
							<Label text="USERINFO" labelFor="userInfo"/>
							<MultiInput id="userInfo" valueHelpRequest="onValueHelpRequest" valueHelpOnly="false" />
						</form:content>
					</form:SimpleForm>
				</layout:content>
			</layout:Grid>
		</content>
	</Page>
</mvc:View>
