			<l:VerticalLayout
				class="sapUiContentPadding"
				width="100%">
				<l:content>
					<HBox
						fitContainer="true"
						alignItems="Stretch">
						<items>
							<VBox
								fitContainer="true">
								<layoutData>
									<FlexItemData growFactor="1" />
								</layoutData>
								<viz:VizFrame id="chart1" width="320px" uiConfig="{applicationSet:'fiori'}" vizType="column"
									vizProperties="{ 
										plotArea:{ 
											dataLabel:{visible: false},
											barSpacing: 0.2,
											dataPointSize: {
												max:10,
												min:10
											}
										}, 
										title:{text:'前期'}, 
										valueAxis:{
											title: {visible: false},
											scale:{
												fixedRange:true,
												minValue:0,
												maxValue:100
											}
										},
										categoryAxis:{
											title: {visible: false},
											label: {
												style: {
													fontSize: '10px'
												}
											},
											axisLine: {
												size: 3
											}
										},
										legend:{
											visible: false
										}
									}" selectData="myOnClickHandler"
									renderComplete="handleRenderComplete">
									<viz:dataset>
										<viz.data:FlattenedDataset data="{chartModel1>/}">
											<viz.data:dimensions>
												<viz.data:DimensionDefinition name="key" value="{chartModel1>key}"/>
											</viz.data:dimensions>
											<viz.data:measures>
												<viz.data:MeasureDefinition name="value" value="{chartModel1>value}"/>
											</viz.data:measures>
										</viz.data:FlattenedDataset>
									</viz:dataset>
									<viz:feeds>
										<viz.feeds:FeedItem uid="categoryAxis" type="Dimension" values="key"/>
										<viz.feeds:FeedItem uid="valueAxis" type="Measure" values="value"/>
									</viz:feeds>
								</viz:VizFrame>
							</VBox>
							<VBox
								fitContainer="true">
								<layoutData>
									<FlexItemData growFactor="1" />
								</layoutData>
								<viz:VizFrame id="chart2" width="370px" uiConfig="{applicationSet:'fiori'}" vizType="line"
									vizProperties="{ 
										plotArea:{ 
											dataLabel:{visible: true} 
										}, 
										title:{text:'後期'}, 
										valueAxis:{
											title: {visible: false},
											scale:{
												fixedRange:true,
												minValue:0,
												maxValue:100
											}
										},
										categoryAxis:{
											title: {visible: false},
											label: {
												style: {
													fontSize: '10px'
												}
											}
										},
										legend:{
											visible: false
										}
									}" selectData="myOnClickHandler"
