{
	"_version": "1.12.0",
	"sap.app": {
		"id": "gitTest.GitTest",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponentForManifest",
			"version": "0.0.0"
		},
		"dataSources": {
			"PwC_SF_Prototype": {
				"uri": "/odata/v2/",
				"type": "OData",
				"settings": {
					"localUri": "localService/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "gitTest.GitTest.view.App",
			"type": "XML",
			"async": true,
			"id": "App"
		},
		"dependencies": {
			"minUI5Version": "1.65.6",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "gitTest.GitTest.i18n.i18n"
				}
			},
			"SF_DS": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request",
					"useBatch": false
				},
				"dataSource": "PwC_SF_Prototype",
				"preload": true
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "gitTest.GitTest.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"async": true
			},
			"routes": [
				{
					"pattern": "",
					"name": "Home",
					"target": "Home"
				},
				{
					"pattern": "Chart",
					"name": "Chart",
					"target": "Chart"
				},
				{
					"pattern": "ECEP",
					"name": "ECEP",
					"target": "ECEP"
				},
				{
					"pattern": "Download",
					"name": "Download",
					"target": "Download"
				},
				{
					"pattern": "DownloadAll",
					"name": "DownloadAll",
					"target": "DownloadAll"
				},
				{
					"pattern": "DownloadPerPerson",
					"name": "DownloadPerPerson",
					"target": "DownloadPerPerson"
				},
				{
					"pattern": "DownloadPerPersonal",
					"name": "DownloadPerPersonal",
					"target": "DownloadPerPersonal"
				},
				{
					"pattern": "DownloadEmpJobRelationships",
					"name": "DownloadEmpJobRelationships",
					"target": "DownloadEmpJobRelationships"
				}
			],
			"targets": {
				"Home": {
					"viewId": "Home",
					"viewName": "Home"
				},
				"Chart": {
					"viewId": "Chart",
					"viewName": "Chart"
				},
				"ECEP": {
					"viewId": "ECEP",
					"viewName": "ECEP"
				},
				"Download": {
					"viewId": "Download",
					"viewName": "Download"
				},
				"DownloadAll": {
					"viewId": "DownloadAll",
					"viewName": "DownloadAll"
				},
				"DownloadPerPerson": {
					"viewId": "DownloadPerPerson",
					"viewName": "DownloadPerPerson"
				},
				"DownloadPerPersonal": {
					"viewId": "DownloadPerPersonal",
					"viewName": "DownloadPerPersonal"
				},
				"DownloadEmpJobRelationships": {
					"viewId": "DownloadEmpJobRelationships",
					"viewName": "DownloadEmpJobRelationships"
				}
			}
		}
	},
	"sap.platform.hcp": {
		"uri": "webapp",
		"_version": "1.1.0"
	}
}